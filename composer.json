        "symfony/yaml" : "~3.2",
        "symfony/console" : "~3.2",
        "symfony/http-foundation" : "~3.2",
        "symfony/routing" : "~3.2",
        "monolog/monolog" : "~1.0",
        "pimple/pimple": "~3.0",
        "symfony/event-dispatcher" : "~3.0",
        "twig/twig" : "~1.28"
    },
    "require-dev" : {
        "phpunit/php-code-coverage": "~2.0"
    },
    "autoload" : {
        "psr-4" : {
